<!-- 定义函数 -->
<mapper namespace="com.jp.plan.dao.PlanSeqFunc">
	<select id="nexVal" parameterType="map" statementType="CALLABLE">
		{
		#{p3,mode=OUT,jdbcType=BIGINT}=call nextval(
		#{p1,mode=IN,jdbcType=VARCHAR}
		)
		}
	</select>
	<select id="nexValbyLen" parameterType="map" statementType="CALLABLE">
		{
		#{p3,mode=OUT,jdbcType=BIGINT}=call nexValbyLen(
		#{p1,mode=IN,jdbcType=INTEGER},
		#{p2,mode=IN,jdbcType=VARCHAR}
		)
		}
	</select>
</mapper>